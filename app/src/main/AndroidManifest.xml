<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bodycare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VideoRutinaHombre" android:label="Rutina" android:parentActivityName=".RutinashombreActivity"/>
        <activity android:name=".Historial" android:label="Historial" android:parentActivityName=".ImcActivity"/>
        <activity android:name=".VideoRutinaMujer" android:label="Rutina" android:parentActivityName=".RutinamujerActivity"/>
        <activity android:name=".RutinamujerActivity" android:label="Rutinas de Mujeres" android:parentActivityName=".MainActivity"/>
        <activity android:name=".RutinashombreActivity" android:label="Rutinas de Hombres" android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".RecetaActivity"
            android:label="Detalles de Receta"
            android:parentActivityName=".RecetasActivity" />
        <activity
            android:name=".GuardarRecetaActivity"
            android:label="Agregar Receta"
            android:parentActivityName=".RecetaActivity" />
        <activity
            android:name=".RecetasActivity"
            android:label="Recetas" />
        <activity
            android:name=".ImcActivity"
            android:label="IMC" />
        <activity
            android:name=".MainActivity"
            android:label="Rutinas">
        </activity>
    </application>

</manifest>