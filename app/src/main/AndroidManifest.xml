<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bodycare">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RecetaActivity" android:parentActivityName=".RecetasActivity" android:label="Detalles"></activity>
        <activity android:name=".GuardarRecetaActivity" android:parentActivityName=".RecetaActivity" android:label="Agregar Receta"/>
        <activity android:name=".RecetasActivity" android:label="Recetas"/>
        <activity android:name=".ImcActivity" android:label="IMC"/>
        <activity android:name=".MainActivity" android:label="Rutinas">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>